@Article{naruseDetailedLinkageMap2000,
  title = {A {{Detailed Linkage Map}} of {{Medaka}}, {{Oryzias}} Latipes: {{Comparative Genomics}} and {{Genome Evolution}}},
  shorttitle = {A {{Detailed Linkage Map}} of {{Medaka}}, {{Oryzias}} Latipes},
  author = {Kiyoshi Naruse and Shoji Fukamachi and Hiroshi Mitani and Mariko Kondo and Tomoko Matsuoka and Shu Kondo and Nana Hanamura and Yutaka Morita and Kazuhito Hasegawa and Reiko Nishigaki and Atsuko Shimada and Hironori Wada and Takehiro Kusakabe and Norio Suzuki and Masato Kinoshita and Akira Kanamori and Tokio Terado and Hiroshi Kimura and Masaru Nonaka and Akihiro Shima},
  date = {2000-04-01},
  journaltitle = {Genetics},
  volume = {154},
