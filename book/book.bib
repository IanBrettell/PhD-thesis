@Article{fredmanComplexSNPrelatedSequence2004,
  title = {Complex {{SNP-related}} Sequence Variation in Segmental Genome Duplications},
  author = {David Fredman and Stefan J. White and Susanna Potter and Evan E. Eichler and Johan T. Den Dunnen and Anthony J. Brookes},
  date = {2004-08},
  journaltitle = {Nature Genetics},
  shortjournal = {Nat Genet},
  volume = {36},
  number = {8},
