@Article{katsumuraMedakaPopulationGenome2019,
  title = {Medaka {{Population Genome Structure}} and {{Demographic History Described}} via {{Genotyping-by-Sequencing}}},
  author = {Takafumi Katsumura and Shoji Oda and Hiroshi Mitani and Hiroki Oota},
  date = {2019-01-01},
  journaltitle = {G3 Genes|Genomes|Genetics},
  shortjournal = {G3 Genes|Genomes|Genetics},
  volume = {9},
  number = {1},
  pages = {217--228},
