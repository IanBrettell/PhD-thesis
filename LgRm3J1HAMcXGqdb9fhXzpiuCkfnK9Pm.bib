@article{schaferClimateJournalismChanging2021,
  title = {Climate Journalism in a Changing Media Ecosystem: {{Assessing}} the Production of Climate Change-Related News around the World},
  shorttitle = {Climate Journalism in a Changing Media Ecosystem},
  author = {Schäfer, Mike S. and Painter, James},
