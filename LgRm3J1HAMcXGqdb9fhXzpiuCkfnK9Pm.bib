@article{rowanRapidInexpensiveWholeGenome2015,
  title = {Rapid and {{Inexpensive Whole-Genome Genotyping-by-Sequencing}} for {{Crossover Localization}} and {{Fine-Scale Genetic Mapping}}},
  author = {Rowan, Beth A and Patel, Vipul and Weigel, Detlef and Schneeberger, Korbinian},
