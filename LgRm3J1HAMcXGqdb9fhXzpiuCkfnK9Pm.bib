  keywords = {Carbon storage,Fungal traits,Organic matter content,Soil aggregate dynamics,Soil multifunctionality,Sustainable land management},
  file = {/Users/brettell/Zotero/storage/IF5D46S5/Emilia Hannula_Morriën_2022_Will fungi solve the carbon dilemma.pdf;/Users/brettell/Zotero/storage/DN52YPFU/S001670612200074X.html}
