  author = {Starling, Melissa J. and Branson, Nicholas and Thomson, Peter C. and McGreevy, Paul D.},
  date = {2013-07-01},
  journaltitle = {Behavioural Processes},
  shortjournal = {Behavioural Processes},
  volume = {97},
