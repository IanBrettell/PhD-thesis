@article{bijmaQuantitativeGeneticsIndirect2014,
  title = {The Quantitative Genetics of Indirect Genetic Effects: A Selective Review of Modelling Issues},
  shorttitle = {The Quantitative Genetics of Indirect Genetic Effects},
  author = {Bijma, P.},
  date = {2014-01},
  journaltitle = {Heredity},
  volume = {112},
