  urldate = {2022-03-15},
  abstract = {There are mean differences in complex traits among global human populations. We hypothesize that part of the phenotypic differentiation is due to natural selection. To address this hypothesis, we assess the differentiation in allele frequencies of trait-associated SNPs among African, Eastern Asian, and European populations for ten complex traits using data of large sample size (up to \textasciitilde 405,000). We show that SNPs associated with height (\$\$P = 2.46 \textbackslash times 10\^\{ - 5\}\$\$), waist-to-hip ratio (\$\$P = 2.77 \textbackslash times 10\^\{ - 4\}\$\$), and schizophrenia (\$\$P = 3.96 \textbackslash times 10\^\{ - 5\}\$\$) are significantly more differentiated among populations than matched “control” SNPs, suggesting that these trait-associated SNPs have undergone natural selection. We further find that SNPs associated with height (\$\$P = 2.01 \textbackslash times 10\^\{ - 6\}\$\$) and schizophrenia (\$\$P = 5.16 \textbackslash times 10\^\{ - 18\}\$\$) show significantly higher variance in linkage disequilibrium (LD) scores across populations than control SNPs. Our results support the hypothesis that natural selection has shaped the genetic differentiation of complex traits, such as height and schizophrenia, among worldwide populations.},
  issue = {1},
  langid = {english},
  keywords = {Evolutionary genetics,Genetic association study},
  file = {/Users/brettell/Zotero/storage/7PKSP89L/Guo et al_2018_Global genetic differentiation of complex traits shaped by natural selection in.pdf;/Users/brettell/Zotero/storage/EEDXF8WF/s41467-018-04191-y.html}
