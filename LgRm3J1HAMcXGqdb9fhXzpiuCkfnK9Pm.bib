  keywords = {Epigenetics,Memory,Nematodes,Small RNAs,Transgenerational inheritance},
  file = {/Users/brettell/Zotero/storage/XCN52RT5/Miska_Rechavi_2021_Can brain activity transmit transgenerationally.pdf;/Users/brettell/Zotero/storage/Q5YLQBGC/S0070215321000272.html}
